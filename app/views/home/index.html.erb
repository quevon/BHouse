<%if current_tenant %>
    <h1>Tenant Page</h1>
    <%= image_tag current_tenant.image_url if current_tenant.image %>
    <%= link_to "Edit Profile" , edit_tenant_registration_path(current_tenant)%>
    <h1>Hello <%= current_tenant.email%></h1>
    <%= button_to "logout", destroy_tenant_session_path, method: :delete  %>
<%elsif current_owner %>
    <%# <%= link_to "Edit Profile" , edit_owner_registration_path(current_owner)%> 
   

    <div class ="container">
        <div class="navigation">
            <ul>
                <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="home-outline"></ion-icon></span>
                        <span class="title">Bhouse App</span>
                    </a>
                </li>
                 <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="home-outline"></ion-icon></span>
                        <span class="title">Dashboard</span>
                    </a>
                </li>
                 <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="person-outline"></ion-icon></span>
                        <span class="title">Customers</span>
                    </a>
                </li>
                 <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="chatbubble-outline"></ion-icon></span>
                        <span class="title">Message</span>
                    </a>
                </li>
                 <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="help-outline"></ion-icon></span>
                        <span class="title">Help</span>
                    </a>
                </li>
                  <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="settings-outline"></ion-icon></span>
                        <span class="title">Settings</span>
                    </a>
                </li>
                  <li>
                    <a href="#">
                        <span class="icon"><ion-icon name="lock-closed-outline"></ion-icon></span>
                        <span class="title">Password</span>
                    </a>
                </li>
                <li>
                    <%= link_to '<span class="icon"><ion-icon name="log-out-outline"></ion-icon></span>'.html_safe + '<span class="title">Sign Out</span>'.html_safe, destroy_owner_session_path, method: :delete  %>
                </li>
            </ul>
    </div>

    <div class="main">
        <div class="topbar">
            <div class="toggle">
                <ion-icon name="menu-outline"></ion-icon>
            </div>
            <div class="search">
                <label>
                    <input type="text" placeholder="Search here">
                    <ion-icon name="search-outline"></ion-icon>
                </label>
            </div>
              <div class="user">
                <div class="username">
                    <span><%= current_owner.firstname%></span>
                    <span><%= current_owner.lastname%></span>
                </div>
                <div class="userprofile">
                    <%= image_tag current_owner.image_url if current_owner.image %>
                </div>
            </div>
          
        </div>
    </div>
<% else %>
    <h1>Home Page</h1>
    <p>You are not signed in.</p>
    <%= link_to "Sign_up as Tenant" , new_tenant_registration_path%>
    <br/>
    <%= link_to "Sign_up as Owner" , new_owner_registration_path%>
    <br/>
    <%= link_to "Login as Tenant", new_tenant_session_path%>
    <br/>
    <%= link_to "Login as Owner", new_owner_session_path%>
    <br/>
    <%= link_to "Login as Admin", new_admin_user_session_path%>
<% end %>